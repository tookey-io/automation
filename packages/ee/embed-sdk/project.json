{
  "name": "ee-embed-sdk",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ee/embed-sdk/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/ee/embed-sdk",
        "main": "packages/ee/embed-sdk/src/index.ts",
        "tsConfig": "packages/ee/embed-sdk/tsconfig.lib.json",
        "assets": []
      }
    },
    "bundle": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "web",
        "compiler": "tsc",
        "outputFileName": "bundled.js",
        "outputPath": "dist/packages/ee/embed-sdk",
        "main": "packages/ee/embed-sdk/src/index.ts",
        "tsConfig": "packages/ee/embed-sdk/tsconfig.lib.json",
        "assets": [],
        "webpackConfig": "packages/ee/embed-sdk/webpack.config.js",
        "isolatedConfig": true,
        "generatePackageJson": true,
        "babelUpwardRootMode": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/ee/embed-sdk/**/*.ts",
          "packages/ee/embed-sdk/package.json"
        ]
      }
    }
  },
  "tags": []
}